				{
					"name": "Create new booking",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData=pm.response.json();\r",
									"pm.environment.set(\"b_id\", jsonData.bookingid);\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"pm.test(\"bookingid is a number\", function () {\r",
									"pm.expect(jsonData.bookingid).to.be.a('number');\r",
									"});\r",
									"\r",
									"pm.test(\"firstname and lastname are strings\", function () {\r",
									"    pm.expect(jsonData.booking.firstname).to.be.a('string');\r",
									"    pm.expect(jsonData.booking.lastname).to.be.a('string');\r",
									"    });\r",
									"pm.test(\"totalprice is a number\", function () {\r",
									"    pm.expect(jsonData.booking.totalprice).to.be.a('number');\r",
									"    });\r",
									"pm.test(\"Check-in and check-out are valid YYYY-MM-DD dates\", function () {\r",
									"    const checkin = jsonData.booking.bookingdates.checkin;\r",
									"    const checkout = jsonData.booking.bookingdates.checkout;\r",
									"\r",
									"    pm.expect(checkin).to.match(/^\\d{4}-\\d{2}-\\d{2}$/);\r",
									"    pm.expect(checkout).to.match(/^\\d{4}-\\d{2}-\\d{2}$/);\r",
									"\r",
									"    pm.expect(Date.parse(checkin)).to.not.be.NaN;\r",
									"    pm.expect(Date.parse(checkout)).to.not.be.NaN;\r",
									"});\r",
									""
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
